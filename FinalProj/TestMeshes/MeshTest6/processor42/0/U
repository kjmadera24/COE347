FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    constant (1 0 0);
        value           uniform (1 0 0);
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        value           nonuniform 0();
    }
    wall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    wedge1
    {
        type            wedge;
    }
    wedge2
    {
        type            wedge;
    }
    axis
    {
        type            empty;
    }
    procBoundary42to36
    {
        type            processor;
        value           uniform (1 0 0);
    }
    procBoundary42to37
    {
        type            processor;
        value           uniform (1 0 0);
    }
    procBoundary42to43
    {
        type            processor;
        value           uniform (1 0 0);
    }
}


